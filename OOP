#criar a classe
class Funcionarios:
    def __init__(self, nome, sobrenome, data_nascimento):
        self.nome = nome
        self.sobrenome = sobrenome
        self.data_nascimento = data_nascimento

#criar o objeto
usuario1 = Funcionarios('Elena', 'Cabral', '25/12/2009')
usuario2 = Funcionarios('Jo√£o', 'Silva', '25/12/2009')
usuario3= Funcionarios('Severina','Medeiros','25/12/2004')

#print
print(usuario1.nome)
print(usuario2.sobrenome)
print(usuario3.data_nascimento)
